@model PatientListViewModel

@foreach (var patient in Model.Patients)
{
<ul class="list-group">
    <li class="list-group-item active">Patient @patient.PatientId</li>
    <li class="list-group-item">@patient.FirstName</li>
    <li class="list-group-item">@patient.LastName</li>
    <li class="list-group-item">@patient.PhoneNumber.ToString()</li>
    <li class="list-group-item">@patient.Mail</li>
</ul>
<div>

</div>
}
